<%layout("/layouts/boilerplate") %>


    <div class="row row-cols-lg-3 row-cols-md-3 row-cols-sm-1">

        <% for(i of alllist){%>
            <a href="/listings/<%=i._id %> " class="link">
                <div class="card col" >
                    <img src="<%=i.image.url %>" class="card-img-top" alt="listing image" height="400rem">
                   
                    <div class="card-body">
                      <p class="card-text"> <b><%=i.tittle %></b> <br>
                        &#x20B9;<%= i.price%>/night
                    </p>
                    </div>
                  </div>
                

            </a>
            
              <%}%>
        

    </div>
    


  
<form method="get" action="/listings/new">
    <br>
    <button class="btn btn-dark">Create New </button>
    <br><br><br>
</form>


